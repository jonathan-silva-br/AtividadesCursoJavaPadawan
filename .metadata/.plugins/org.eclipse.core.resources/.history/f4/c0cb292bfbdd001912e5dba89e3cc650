
public class Conta {
	private double saldo;
	
	public double getSaldo() {
		return saldo;
	}

	public void deposito(double valor) {
		if (valor > 0) {
			this.saldo += valor;
		}
	}
	
	public double saque(double valor) {
		if (getSaldo() >= valor) {
			this.saldo -= valor;
			return valor;
		}
	}
}
